#!/bin/bash
#Generated by JetBrains Toolbox 3.1.0.62320 at 2025-11-20T20:14:50

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/marane4/Applications/PyCharm.app/Contents/MacOS/pycharm" $wait --args "${intellij_args[@]}"