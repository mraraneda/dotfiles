#!/bin/bash
#Generated by JetBrains Toolbox 3.2.0.65851 at 2025-12-19T13:21:48

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/marane4/Applications/RustRover.app/Contents/MacOS/rustrover" $wait --args "${intellij_args[@]}"